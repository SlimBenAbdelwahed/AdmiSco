{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Etablissement</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nometablissement</th>
                <td>{{ entity.nomEtablissement }}</td>
            </tr>
            <tr>
                <th>Adresseetablissement</th>
                <td>{{ entity.adresseEtablissement }}</td>
            </tr>
            <tr>
                <th>Teletablissement</th>
                <td>{{ entity.telEtablissement }}</td>
            </tr>
            <tr>
                <th>Faxetablissement</th>
                <td>{{ entity.faxEtablissement }}</td>
            </tr>
            <tr>
                <th>Typeetablissement</th>
                <td>{{ entity.typeEtablissement }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('etablissement') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('etablissement_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
